{
  "name": "nexjs-project-starter",
  "version": "1.0.0",
  "description": "Nexjs, SCSS, TypeScript, Jest, ESLint, and Next plugins.",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "test": "jest",
    "lint": "eslint src/",
    "types": "run scss && tsc --noemit",
    "scss": "pnpify tsm src --implementation sass --nameFormat none --exportType default --aliasPrefixes.~ node_modules/",
    "lint-all": "run types && run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkamenoko/nexjs-project-starter.git"
  },
  "author": "",
